import os
import time
m = 4.06*1024
net = r"d:\bb"
dir = r"d:\aa"

def up():
    begin = time.time()
    cmd = "xcopy /s /e /q /i /y %s %s"%(dir,net)
    # cmd = "xcopy /s /e /q /i /y %s %s"%(net,dir)
    # cmd = "cp -r %s %s"%(net,dir)
    # cmd = "cp -r %s %s"%(net,dir)
    os.system(cmd)
    end = time.time()
    use = end - begin
    speed = m/use
    print(speed)

up()